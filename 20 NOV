
var str1 = "kandy";
var str2 = 'mandy';
var str3 = `sandy`;
var str4 = new String("dinga");

console.log(typeof str1);  
console.log(typeof str2);
console.log(typeof str3);  //string
console.log(typeof str4.valueOf);  //function

var num =10;
num = 10.5;
num = 10/5;
var num1 = new Number(2);

console.log(typeof num); //number
console.log(typeof num1.valueOf);   //function

var username;
console.log(typeof username);  //undefined

var testnull= null;
console.log(typeof testnull);  //object

var bool = true;
var bool1 = new Boolean();
console.log(typeof bool);  //boolean 
console.log(typeof bool1);  //object

var array = ["java","python",undefined,null,{},function() {},true ];
console.log(array.length);  //7
console.log(array[0]);  //java

for( var i=0;i<=array.length;i++)   //iterating all
  {
    console.log(array[i])
  };

